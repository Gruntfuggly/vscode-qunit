{
  "name": "vscode-qunit",
  "displayName": "vscode-qunit",
  "description": "Integrate qunit (JavaScript unit test framework) with Visual Studio Code",
  "version": "0.0.1",
  "publisher": "Gruntfuggly",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "qunit-test-results",
          "name": "Qunit Test Results"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-qunit.runTests",
        "title": "Run qunit tests"
      }
    ]
  },
  "scripts": {},
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "qunit": "^2.4.1",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "zombie": "^5.0.7"
  }
}
