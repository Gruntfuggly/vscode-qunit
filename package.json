{
  "name": "vscode-qunit",
  "displayName": "vscode-qunit",
  "icon": "resources/qunit.png",
  "description": "Integrate qunit (JavaScript unit test framework) with Visual Studio Code",
  "version": "0.0.2",
  "publisher": "Gruntfuggly",
  "repository": "https://github.com/Gruntfuggly/qunit-vscode",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "qunit-test-results",
          "name": "QUnit Test Results"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vscode-qunit.refresh",
          "when": "view == qunit-test-results",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-qunit.runTests",
        "title": "Run tests",
        "category": "QUnit"
      },
      {
        "command": "vscode-qunit.refresh",
        "title": "Refresh",
        "category": "QUnit",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Trigger Task on Save configuration",
      "properties": {
        "vscode-qunit.file": {
          "type": "string",
          "description": "HTML file to execute tests"
        },
        "vscode-qunit.showExecutionTimes": {
          "type": "boolean",
          "description": "Add display of executiom times",
          "default": "false"
        }
      }
    }
  },
  "scripts": {},
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "vscode": "^1.0.0",
    "zombie": "^5.0.7"
  }
}